<% layout('layouts/boilerplate') %> <% for (let user of users) {%>
<div class="card" mb-3>
  <div class="row">
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title"><%= user.username %></h5>
        <p class="card-text"><%= user.email %></p>
      </div>
    </div>
  </div>
</div>
<% } %>
<br />
<div>
  <a class="btn btn-info" href="/form">Add User</a>
</div>
